@{
    ViewData["Title"] = "Ana Sayfa";

}


<div class="dock-target home-fixed-top">
    <nav class="navbar fixed-top">
        <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
            <p><a class="btn btn-sm btn-primary" href="/Identity/Account/Register">Login/Register</a></p>
        </div>
    </nav>
    <div id="myCarousel" class="carousel slide mb-6" data-bs-ride="carousel">
        <div class="carousel-indicators">
            <button type="button" data-bs-target="#myCarousel" data-bs-slide-to="0" class="active" aria-label="Slide 1" aria-current="true"></button>
            <button type="button" data-bs-target="#myCarousel" data-bs-slide-to="1" aria-label="Slide 2" class=""></button>
            <button type="button" data-bs-target="#myCarousel" data-bs-slide-to="2" aria-label="Slide 3" class=""></button>
        </div>
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img src="img/carausel_first-photoaidcom-blur.png" />
                <div class="container">
                    <div class="carousel-caption text-start">
                        <h1>Graphics and Tables</h1>
                        <p class="opacity-75">You can visualize your transactions with the help of tables and graphs.</p>
                        <p><a class="btn btn-lg btn-primary" href="/Identity/Account/Register">Sign up today</a></p>
                    </div>
                </div>
            </div>
            <div class="carousel-item">
                <img src="img/carausel_second-photoaidcom-blur.png" />
                <div class="container">
                    <div class="carousel-caption">
                        <h1>Transactions</h1>
                        <p>You can add new transactions by categories and define them as income and expenses.</p>

                    </div>
                </div>
            </div>
            <div class="carousel-item">
                <img src="img/carausel_third-photoaidcom-blur.png" class="carousel-img" />
                <div class="container">
                    <div class="carousel-caption">
                        <h1>Categories</h1>
                        <p>You can define categories at any time.</p>

                    </div>
                </div>
            </div>
        </div>
        <button class="carousel-control-prev" type="button" data-bs-target="#myCarousel" data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
        </button>
        <button class="carousel-control-next" type="button" data-bs-target="#myCarousel" data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
        </button>
    </div>

    <div class="container marketing">

        <!-- Three columns of text below the carousel -->
        <div class="row carousel-row">
            <div class="col-lg-6">
                
                <img src="img/App_Store_(iOS).svg.png" width="80" height="80" />
                <h4 class="fw-normal">App Store</h4>
                <p><a class="btn btn-secondary" href="#">Download »</a></p>
            </div><!-- /.col-lg-4 -->
            <div class="col-lg-6">
                <img src="img/Google-Play.png" width="80" height="80" />
                <h4 class="fw-normal">Google Play </h4>
                <p><a class="btn btn-secondary" href="#">Download »</a></p>
            </div><!-- /.col-lg-4 -->

        </div><!-- /.row -->

    </div>

    <div class="container card-container">
        <div class="row">
            <div class="col-md-3">
                <div class="card-home">
                    <div class="card-home-body">
                        <p class="card-text">This is by far the best expense tracker app I've found.</p>
                        <h6>RJ</h6>
                        <div class="stars">
                            <i class="fa-solid fa-star"></i>
                            <i class="fa-solid fa-star"></i>
                            <i class="fa-solid fa-star"></i>
                            <i class="fa-solid fa-star"></i>
                            <i class="fa-solid fa-star"></i>
                        </div>

                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="card-home">
                    <div class="card-home-body">
                        <p class="card-text">I don't consider myself very "extravagant" but thanks to this app I can manage my money much better.</p>
                        <h6>Jack</h6>
                        <div class="stars">
                            <i class="fa-solid fa-star"></i>
                            <i class="fa-solid fa-star"></i>
                            <i class="fa-solid fa-star"></i>
                            <i class="fa-solid fa-star"></i>
                            <i class="fa-solid fa-star"></i>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="card-home">
                    <div class="card-home-body">
                        <p class="card-text">I've been using it for 2 months, I'm more frugal now.</p>
                        <h6>Jasmine</h6>
                        <div class="stars">
                            <i class="fa-solid fa-star"></i>
                            <i class="fa-solid fa-star"></i>
                            <i class="fa-solid fa-star"></i>
                            <i class="fa-solid fa-star"></i>
                            <i class="fa-solid fa-star-half-stroke"></i>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="card-home">
                    <div class="card-home-body">
                        <p class="card-text">Love Expense Tracker! It keeps me on track and mindful about where and how much of money is going out. It is very easy to use.</p>
                        <h6>Lily</h6>
                        <div class="stars">
                            <i class="fa-solid fa-star"></i>
                            <i class="fa-solid fa-star"></i>
                            <i class="fa-solid fa-star"></i>
                            <i class="fa-solid fa-star"></i>
                            <i class="fa-solid fa-star"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>


    <div class="container-create-account">
        <div class="row">
            <div>
                <h3>
                    Try the free version of Expense Tracker
                    <br>
                    and start budgeting today.
                </h3>
            </div>
            <div class="create-account">
                <a href="/Identity/Account/Register" class="btn btn-lg btn-primary">
                    Create Your Free Account
                </a>
            </div>
        </div>
    </div>
